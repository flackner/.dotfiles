{
  "security.workspace.trust.untrustedFiles": "newWindow",
  "terminal.integrated.scrollback": 1000000,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "search.searchEditor.defaultNumberOfContextLines": 0,
  "workbench.reduceMotion": "off",
  "multiCommand.commands": [
    {
      "command": "multiCommand.upWithFocus",
      "sequence": [
        {
          "command": "list.focusUp"
        },
        {
          "command": "list.selectAndPreserveFocus"
        }
      ]
    },
    {
      "command": "multiCommand.downWithFocus",
      "sequence": [
        {
          "command": "list.focusDown"
        },
        {
          "command": "list.selectAndPreserveFocus"
        }
      ]
    }
  ],
  "editor.smoothScrolling": true,
  "workbench.iconTheme": "file-icons",
  "editor.semanticHighlighting.enabled": true,
  "editor.fontFamily": "JetBrainsMono NF",
  "editor.fontLigatures": true,
  "faybcontrol.distance": 5,
  "faybcontrol.offset": 5,
  "editor.minimap.enabled": false,
  "cmake.configureOnOpen": false,
  "security.workspace.trust.enabled": false,
  "cmake.showOptionsMovedNotification": false,
  "json.format.keepLines": true,
  "fortran.formatting.formatter": "fprettify",
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "cmake.generator": "Unix Makefiles",
  "pylint.args": [
    "\"pylint.args\": [\"--max-line-length=132\"]"
  ],
  "editor.renderLineHighlight": "none",
  "C_Cpp.inactiveRegionForegroundColor": "#8B8772",
  "python.languageServer": "Pylance",
  "workbench.colorTheme": "faybtheme_violet",
  "workbench.colorCustomizations": {
    // put your development theme file here
  }

}