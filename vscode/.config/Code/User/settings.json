{
  "C_Cpp.inactiveRegionForegroundColor": "#8B8772",
  "cmake.configureOnOpen": false,
  "cmake.generator": "Unix Makefiles",
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.fontFamily": "JetBrainsMono NF",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "comments": false,
    "other": false,
    "strings": false
  },
  "editor.rename.enablePreview": false,
  "editor.semanticHighlighting.enabled": true,
  "editor.smoothScrolling": true,
  "editor.suggestOnTriggerCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "faybcontrol.distance": 20,
  "faybcontrol.offset": 5,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.enable": {
    "markdown": true,
    "plaintext": true,
    "scminput": true
  },
  "julia.enableTelemetry": false,
  "julia.symbolCacheDownload": true,
  "json.format.keepLines": true,
  "multiCommand.commands": [
    {
      "command": "multiCommand.upWithFocus",
      "sequence": [
        {
          "command": "list.focusUp"
        },
        {
          "command": "list.selectAndPreserveFocus"
        }
      ]
    },
    {
      "command": "multiCommand.downWithFocus",
      "sequence": [
        {
          "command": "list.focusDown"
        },
        {
          "command": "list.selectAndPreserveFocus"
        }
      ]
    },
    {
      "command": "multiCommand.focusFirstEditorGroup",
      "sequence": [
        {
          "command": "workbench.action.maximizeEditorHideSidebar"
        },
        {
          "command": "workbench.action.closePanel"
        },
        {
          "command": "workbench.action.closeSidebar"
        },
        {
          "command": "workbench.action.activityBarLocation.hide"
        },
        {
          "command": "workbench.action.focusAuxiliaryBar"
        },
        {
          "command": "workbench.action.focusFirstEditorGroup"
        },
        {
          "command": "faybcontrol.placeCursorUp"
        }
      ]
    },
    {
      "command": "multiCommand.focusSecondEditorGroup",
      "sequence": [
        {
          "command": "workbench.action.maximizeEditorHideSidebar"
        },
        {
          "command": "workbench.action.closePanel"
        },
        {
          "command": "workbench.action.closeSidebar"
        },
        {
          "command": "workbench.action.activityBarLocation.hide"
        },
        {
          "command": "workbench.action.focusAuxiliaryBar"
        },
        {
          "command": "workbench.action.focusSecondEditorGroup"
        },
        {
          "command": "faybcontrol.placeCursorUp"
        }
      ]
    },
    {
      "command": "multiCommand.focusTerminal",
      "sequence": [
        {
          "command": "workbench.action.closeSidebar"
        },
        {
          "command": "workbench.action.closeAuxiliaryBar"
        },
        {
          "command": "workbench.action.activityBarLocation.hide"
        },
        {
          "command": "workbench.action.closePanel"
        },
        {
          "command": "workbench.action.toggleMaximizedPanel"
        }
      ]
    }
  ],
  "python.languageServer": "Pylance",
  "search.searchEditor.defaultNumberOfContextLines": 0,
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "terminal.integrated.commandsToSkipShell": [
    "language-julia.interrupt",
    "workbench.action.focusSecondEditorGroup",
    "workbench.action.focusFirstEditorGroup",
    "workbench.action.quickOpen",
    "workbench.action.increaseViewSize",
    "workbench.action.decreaseViewSize"
  ],
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.env.linux": { },
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.scrollback": 1000000,
  "terminal.integrated.allowChords": false,
  "terminal.integrated.fontFamily": "JetBrainsMono NF",
  "window.menuBarVisibility": "toggle",
  "workbench.colorCustomizations": {
    //"editor.background": "#1e181e", // Editor background color.
  },
  "workbench.colorTheme": "faybtheme_violet",
  "workbench.editor.enablePreview": false,
  "workbench.editor.revealIfOpen": false,
  "workbench.editor.showTabs": "none",
  "workbench.reduceMotion": "off",
  "workbench.startupEditor": "none",
  "github.copilot.nextEditSuggestions.fixes": false,
  "workbench.panel.showLabels": false,
  "workbench.secondarySideBar.showLabels": false,
  "extensions.ignoreRecommendations": true,
  "workbench.activityBar.location": "hidden"
}